{
  "extends": [
    "eslint-config-airbnb-base"
  ],
  "env": {
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": "2022"
  },
  "rules": {
    "prefer-regex-literals": 0,
    "quote-props": 0,
    "import/no-dynamic-require": 0,
    "prefer-destructuring": 0,
    "consistent-return": 0,
    "class-methods-use-this": 0,
    "max-classes-per-file": 0,
    "max-len": 0,
    "lines-between-class-members": 0,
    "no-await-in-loop": 0,
    "no-bitwise": 0,
    "no-cond-assign": 0,
    "no-empty-function": 0,
    "no-plusplus": 0,
    "no-promise-executor-return": 0,
    "no-underscore-dangle": 0,
    "no-unused-expressions": 0,
    "no-unused-vars": [
      "error",
      {
        "varsIgnorePattern": "^debug|ok|eq|neq|deq|match|throws|rejects|describe|it|before|after|beforeEach|afterEach$",
        "argsIgnorePattern": "^_\\w+"
      }
    ],
    "no-use-before-define": 0,
    "object-curly-newline": 0,
    "padded-blocks": 0,
    "valid-typeof": 0,
    "arrow-body-style": 0,
    "arrow-parens": 0,
    "camelcase": 0,
    "comma-dangle": 0,
    "eol-last": 0,
    "func-names": 0,
    "function-paren-newline": 0,
    "global-require": 0,
    "import/extensions": 0,
    "import/newline-after-import": 0,
    "import/order": 0,
    "indent": 0,
    "no-confusing-arrow": 0,
    "no-console": 0,
    "no-dynamic-require": 0,
    "no-else-return": 0,
    "no-extra-semi": 0,
    "no-mixed-spaces-and-tabs": 0,
    "no-multiple-empty-lines": 0
  }
}