-- add enum
DO $$
BEGIN
  BEGIN
    CREATE TYPE {{identifier name}} AS ENUM (
      {{#values}}
      {{literal this}}{{#unless @last}},{{/unless}}
      {{/values}}
    );
  EXCEPTION
    WHEN duplicate_object
    THEN RAISE EXCEPTION 'Enum '{{literal name}}' already exists';
  END;
END;
$$ LANGUAGE plpgsql;